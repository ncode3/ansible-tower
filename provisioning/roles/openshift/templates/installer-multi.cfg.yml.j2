ansible_callback_facts_yaml: /root/.config/openshift/.ansible/callback_facts.yaml
ansible_inventory_path: /root/.config/openshift/hosts
ansible_log_path: /tmp/ansible.log
deployment:
  ansible_ssh_user: root
  hosts:
  - connect_to: {{ ansible_default_ipv4.address }}
    hostname: {{ ansible_nodename }}
    ip: {{ ansible_default_ipv4.address }}
    node_labels: '{''region'': ''infra''}'
    public_hostname: {{ ansible_nodename }}
    public_ip: {{ ansible_default_ipv4.address }} 
    roles:
    - master
    - etcd
    - node
    - storage
  - connect_to: {{ hostvars['ose-cluster-node-1.dev.maskedadmin.com']['ansible_default_ipv4']['address'] }}
    hostname: {{ hostvars['ose-cluster-node-1.dev.maskedadmin.com']['ansible_nodename'] }}
    ip: {{ hostvars['ose-cluster-node-1.dev.maskedadmin.com']['ansible_default_ipv4']['address'] }}
    node_labels: '{''region'': ''infra''}'
    public_hostname: {{ hostvars['ose-cluster-node-1.dev.maskedadmin.com']['ansible_nodename'] }}
    public_ip: {{ hostvars['ose-cluster-node-1.dev.maskedadmin.com']['ansible_default_ipv4']['address'] }}
    roles:
    - node
    containerized: false
  - connect_to: {{ hostvars['ose-cluster-node-2.dev.maskedadmin.com']['ansible_default_ipv4']['address'] }}
    hostname: {{ hostvars['ose-cluster-node-2.dev.maskedadmin.com']['ansible_nodename'] }}
    ip: {{ hostvars['ose-cluster-node-2.dev.maskedadmin.com']['ansible_default_ipv4']['address'] }}
    node_labels: '{''region'': ''infra''}'
    public_hostname: {{ hostvars['ose-cluster-node-2.dev.maskedadmin.com']['ansible_nodename'] }}
    public_ip: {{ hostvars['ose-cluster-node-2.dev.maskedadmin.com']['ansible_default_ipv4']['address'] }}
    roles:
    - node
    containerized: false
  master_routingconfig_subdomain: 'ose.maskedadmins.com'
  proxy_exclude_hosts: ''
  proxy_http: ''
  proxy_https: ''
  roles:
    etcd: {}
    master: {}
    node: {}
    storage: {}
variant: openshift-enterprise
variant_version: '{{ ocp_variant }}'
version: v2
